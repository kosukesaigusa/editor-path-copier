{
  "name": "editor-path-copier",
  "displayName": "Editor Path Copier",
  "description": "Easily copy file paths from the editor toolbar with one click",
  "version": "0.0.2",
  "publisher": "kosukesaigusa",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "path",
    "copy",
    "clipboard",
    "file",
    "toolbar",
    "utility",
    "copy path",
    "file path",
    "absolute path",
    "relative path",
    "path copy",
    "editor",
    "productivity"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kosukesaigusa/editor-path-copier.git"
  },
  "bugs": {
    "url": "https://github.com/kosukesaigusa/editor-path-copier/issues"
  },
  "homepage": "https://github.com/kosukesaigusa/editor-path-copier#readme",
  "license": "MIT",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "editor-path-copier.copyAbsolutePath",
        "title": "Copy Absolute Path",
        "icon": "$(copy)"
      },
      {
        "command": "editor-path-copier.copyRelativePath",
        "title": "Copy Relative Path",
        "icon": "$(copy)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "editor-path-copier.copyAbsolutePath",
          "when": "resourceScheme == file",
          "group": "navigation"
        },
        {
          "command": "editor-path-copier.copyRelativePath",
          "when": "resourceScheme == file",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "biome lint --write",
    "format": "biome format --write",
    "check": "biome check --write",
    "package": "vsce package"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^2.22.0",
    "typescript": "^4.9.4"
  }
}
